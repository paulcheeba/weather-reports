uid(\"Compendium.weather-reports.weather-macros.y1Xy3fs5jYctO3FX\");\n        return macro.execute();\n        },\n      icon: `<i class=\"fa-solid fa-sun-cloud\"></i>`\n    },\n    summer: {\n      label: \"Summer Weather\",\n      callback: async() => {\n        ui.notifications.info(\"Oceanic Summer Weather rolled!\");\n        const macro = await fromUuid(\"Compendium.weather-reports.weather-macros.0Js56xe7TzYOPLXv\");\n        return macro.execute();\n        },\n      icon: `<i class=\"fa-solid fa-sun-bright\"></i>`\n    },\n    fall: {\n      label: \"Fall Weather\",\n      callback: async() => {\n        ui.notifications.info(\"Oceanic Fall Weather rolled!\");\n        const macro = await fromUuid(\"Compendium.weather-reports.weather-macros.Tf6Z8VJB2qVUOH1X\");\n        return macro.execute();\n        },\n      icon: `<i class=\"fa-solid fa-wind\"></i>`\n    },\n    winter: {\n      label: \"Winter Weather\",\n      callback: async() => {\n        ui.notifications.info(\"Oceanic Winter Weather rolled!\");\n        const macro = await fromUuid(\"Compendium.weather-reports.weather-macros.82nNLyd0yvgAU2v6\");\n        return macro.execute();\n        },\n      icon: `<i class=\"fa-solid fa-snowflake\"></i>`\n    },\n}\n}).render(true);","ownership":{"default":0,"Rvy5E942LzI7U7KL":3},"flags":{"core":{"sourceId":"Macro.HerQnRIlggpHZDmj"},"cf":{"id":"temp_j4pd5jn9n6r"}},"_stats":{"systemId":"dnd5e","systemVersion":"2.0.3","coreVersion":"10.291","createdTime":1666643730285,"modifiedTime":1679788337880,"lastModifiedBy":"Rvy5E942LzI7U7KL"},"folder":null,"sort":0,"_id":"iKBNwujiCnxSrcZH"}
{"name":"Arctic Spring WR","type":"script","author":"Rvy5E942LzI7U7KL","img":"modules/weather-reports/img/Weather_Effects_Arctic.webp","scope":"global","command":"//WR Regional Seasonal Flavour\nconst regionSeason = \"Arctic Spring\";\n//Arctic\n    //Arctic seasons\n    const arSp = \"Compendium.weather-reports.weather-rolltables.eiPbSPIWjF1ZUdbq\";//spring\n    const arSu = \"Compendium.weather-reports.weather-rolltables.52jxAe8JF6ocrY8u\";//summer\n    const arFa = \"Compendium.weather-reports.weather-rolltables.vYIks2gLtXGh6Zcz\";//fall\n    const arWi = \"Compendium.weather-reports.weather-rolltables.wugTYqAwBeDcH1Lm\";//winter\n    //Arctic temps\n    const arSpT = \"Compendium.weather-reports.weather-rolltables.TlhsbXMLBWQHPSwS\";//spring temps\n    const arSuT = \"Compendium.weather-reports.weather-rolltables.HSE19R420ouaFLd3\";//summer temps\n    const arFaT = \"Compendium.weather-reports.weather-rolltables.vYIks2gLtXGh6Zcz\";//fall temps\n    const arWiT = \"Compendium.weather-reports.weather-rolltables.nEMHkpsDfygZnc82\";//winter temps\n    \n//point variables to regional seasonal conditions and regional seasonal temps constants\nconst wrCondition = arSp;\nconst wrTemp = arSpT;\n\n//Roll 3 tables and get the results\n//table 1 = regional seasonal conditions and FX\nconst table1 = await fromUuid(wrCondition);\nconst {results: results1} = await table1.roll();\nconst weathercondition = results1[0].getChatText();\nconst fxmaster = results1[1].getChatText();\nvar additional = \"\";\nif(results1[2]) additional = `<br> <b>Additional Weather Effect: </b>${results1[2].getChatText()}`;\n\n//table 2 = regional seasonal tempurature\nconst table2 = await fromUuid(wrTemp);\nconst {results: results2} = await table2.roll();\nconst temps = results2[0].getChatText();\n\n//table 3 = duration\nconst table3 = await fromUuid(\"Compendium.weather-reports.weather-rolltables.swFGhfoMimueFnTz\"); //Duration RT\nconst {results: results3} = await table3.roll();\nconst durdesc = results3[0].getChatText();\nconst duration = Number(results3[0].getChatText().match(/\\d{1,3}/gm)[0]);\n\n//configure simple calendar event\nconst body = weathercondition + '<br>' + fxmaster + '<br> ' + durdesc + additional;\nconst startDate = SimpleCalendar.api.timestampToDate(SimpleCalendar.api.timestamp());\nconst endDate = SimpleCalendar.api.timestampToDate(SimpleCalendar.api.timestamp() + duration * 60 * 60);\nawait SimpleCalendar.api.addNote('Weather Report', body, startDate, endDate);\n\n//variables for chat message\nconst tableWeatherCondition = weathercondition;\nconst tableFXMasterEffect = fxmaster;\nconst tableTemp = temps;\nconst tableDuration = durdesc;\n\n//create chat message\nlet messageTable = `\n<b><h2>WEATHER REPORT:</h2></b>\n<p style=\"font-size:10px;\"><i>${regionSeason} Report</i></p>\n<b>Weather Condition: </b>${tableWeatherCondition}<br>\n<b>FXMaster Effect: </b>${tableFXMasterEffect}<br>\n<b>Tempurature: </b>${tableTemp}<br>\n<b>Weather Duration: </b>${tableDuration}<br>\n${additional}`\n\nlet chatData = {\n    user: game.user._id,\n    speaker: ChatMessage.getSpeaker(),\n    content: messageTable,\n    //whisper: game.users.entities.filter(u => u.isGM).map(u => u._id)\n};\n//send chat message\nChatMessage.create(chatData, {});","flags":{"core":{"sourceId":"Macro.H58j6ubIQgnb8kUQ"},"cf":{"id":"temp_b2nghrl32tk","path":"Weather Reports Macros 0.1.7#/CF_SEP/Arctic","color":"#6fa5b8"}},"_stats":{"systemId":"dnd5e","systemVersion":"2.0.3","coreVersion":"10.291","createdTime":1676156261861,"modifiedTime":1679207855692,"lastModifiedBy":"Rvy5E942LzI7U7KL"},"_id":"iffZX7p5npFrOfPC","folder":null,"sort":0,"ownership":{"default":0,"Rvy5E942LzI7U7KL":3}}
